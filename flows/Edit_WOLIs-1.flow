            <dataType>String</dataType>
            <defaultSelectedChoiceReference>CurrentWOLIStatus</defaultSelectedChoiceReference>
            <fieldText>Status</fieldText>
            <fieldType>DropdownBox</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>AssetPlist</name>
            <choiceReferences>Asset</choiceReferences>
            <choiceReferences>CurrentWOLIAsset</choiceReferences>
